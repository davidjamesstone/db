[
  {
    "name": "owner",
    "type": "BelongsToOne",
    "modelClass": "Person",
    "join": {
      "from": "pet.owner_id",
      "to": "person.id"
    },
    "onDelete": "CASCADE"
  },
  {
    "name": "pets",
    "type": "HasMany",
    "modelClass": "Pet",
    "join": {
      "from": "person1.id",
      "to": "pet.owner_id"
    },
    "onDelete": "CASCADE"
  },
  {
    "name": "movies",
    "type": "ManyToMany",
    "modelClass": "Movie",
    "join": {
      "from": "persons.id",
      "through": {
        "from": "persons_movies.personId",
        "to": "persons_movies.movieId"
      },
      "to": "movies.id"
    },
    "onDelete": "CASCADE"
  },
  {
    "name": "children",
    "type": "HasMany",
    "modelClass": "Person",
    "join": {
      "from": "persons.id",
      "to": "persons.parentId"
    },
    "onDelete": "CASCADE"
  },
  {
    "name": "parent",
    "type": "BelongsToOne",
    "modelClass": "Person",
    "join": {
      "from": "persons.parentId",
      "to": "persons.id"
    },
    "onDelete": "CASCADE"
  }
]